.uploader, .tunnel {
  height: 9vh;
  padding: 0 60rpx 0 40rpx;
  flex-direction:row;
  align-items: center;
  box-sizing: border-box;

}

.uploader-text, .tunnel-text {
  width: 100%;
  line-height: 52px;
  font-size: 34rpx;
  color: #000000;
  overflow: hidden;
  white-space: nowrap;
  text-overflow:ellipsis;
}


.uploader-image {
  width: 100%;
  height: 360rpx;
}


.button1{
  
  width: 50%;
  margin-top: 80rpx;
  background-color: #ffffff;
  color: white;
  border: 1rpx solid #000000;
  border-radius: 98rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;

}
.button1-text {
  
  font-size: 34rpx;
  color: #000000;
}

.icon1{
  
  margin-right:20rpx;
  width:12vw;
  height:5vh;
}

.icon2{
  
  margin-right:30rpx;
  width:55rpx;
  height:50rpx;
}

.icon3{
  margin-top:15rpx;
  width:35rpx;
  height:30rpx;
}

.icon4{
  
  width:45rpx;
  height:45rpx;
}
